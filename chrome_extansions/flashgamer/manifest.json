{
  "name": "Flash Gamer",
  "version": "0.9",
  "description": "This extension 'fixes up' miniclip.com, nitrome.com and many more!",
  "options_page": "options.html",
  "icons": { "128": "icon.png" },
  "content_scripts": [
    {
      "matches": ["http://www.miniclip.com/*"],
      "css": ["miniclip.css"],
      "js": ["miniclip.js"]
    },
    {
      "matches": ["http://www.miniclip.com/games/*/*/"],
      "css": ["giniclip.css"],
      "js": ["giniclip.js"]
    },
    {
      "matches": ["http://nitrome.com/*", "http://www.nitrome.com/*"],
      "js": ["nitrome.js"]
    },
    {
      "matches": ["http://www.kongregate.com/*"],
      "css": ["kong.css"]
    }
    
  ]
}
